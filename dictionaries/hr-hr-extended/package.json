{
  "name": "@cspell/dict-hr-hr-extended",
  "version": "1.0.0",
  "description": "Extended Croatian dictionary  -- Private until verified",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./cspell-ext.json",
    "./cspell": "./cspell-ext.json",
    "./cspell-ext.json": "./cspell-ext.json"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 cspell-tools compile-trie \"src/output.txt\" -o .",
    "test": "head -n 1000 \"src/output.txt\" | cspell -v -c ./cspell-ext.json \"--local=hr\" \"--languageId=*\" stdin",
    "prepublishOnly": "yarn test",
    "prepare": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/streetsidesoftware/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "hr-hr-extended",
    "hr-hr-extended",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streetsidesoftware/cspell-dicts/issues"
  },
  "homepage": "https://github.com/streetsidesoftware/cspell-dicts/blob/master/packages/hr-hr-extended#readme",
  "devDependencies": {},
  "dependencies": {},
  "files": [
    "output.trie.gz",
    "cspell-ext.json",
    "*.js",
    "*.d.ts"
  ]
}
